# name: test/sql/storage/compression/rle_sorted/compression_methods_compatibility.test
# description: Test dictionary compression on a large table
# group: [rle_sorted]
mode skip

# load the DB from disk
load __TEST_DIR__/all_types_storage.db

statement ok
PRAGMA enable_verification

foreach compression <compression>

statement ok
PRAGMA force_compression='${compression}'

statement ok
PRAGMA force_compression_sorting=false

statement ok
CREATE TABLE all_types AS SELECT * FROM test_all_types();

loop i 0 10

statement ok
INSERT INTO all_types SELECT * FROM all_types;

endloop

statement ok
DROP TABLE all_types;

endloop
